@article{arnholdModellingInterplayMultiple2017,
  title = {Modelling the Interplay of Multiple Cues in Prosodic Focus Marking},
  author = {Arnhold, Anja and Kyröläinen, Aki-Juhani},
  date = {2017-03-13},
  journaltitle = {Laboratory Phonology},
  volume = {8},
