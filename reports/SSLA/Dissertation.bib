@article{andruskiPhonationTypesProduction2000,
  title = {Phonation Types in Production of Phonological Tone: The Case of {{Green Mong}}},
  shorttitle = {Phonation Types in Production of Phonological Tone},
  author = {Andruski, Jean E. and Ratliff, Martha},
  date = {2000-12},
