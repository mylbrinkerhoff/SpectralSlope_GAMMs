@incollection{podesvaStanceWindowLanguageRace2016,
  title = {Stance as a {{Window}} into the {{Language-Race Connection}}: {{Evidence}} from {{African American}} and {{White Speakers}} in {{Washington}}, {{DC}}},
  shorttitle = {Stance as a {{Window}} into the {{Language-Race Connection}}},
  booktitle = {Raciolinguistics: {{How Language Shapes Our Ideas About Race}}},
  author = {Podesva, Robert J.},
