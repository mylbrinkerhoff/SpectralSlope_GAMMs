@article{moisikModelingBiomechanicalInfluence2014,
  title = {Modeling the {{Biomechanical Influence}} of {{Epilaryngeal Stricture}} on the {{Vocal Folds}}: {{A Low-Dimensional Model}} of {{Vocal}}–{{Ventricular Fold Coupling}}},
  shorttitle = {Modeling the {{Biomechanical Influence}} of {{Epilaryngeal Stricture}} on the {{Vocal Folds}}},
  author = {Moisik, Scott R. and Esling, John H.},
  date = {2014-04},
  journaltitle = {Journal of Speech, Language, and Hearing Research},
  volume = {57},
