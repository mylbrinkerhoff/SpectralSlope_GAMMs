@article{chaiCheckedSyllablesChecked2022,
  title = {Checked {{Syllables}}, {{Checked Tones}}, and {{Tone Sandhi}} in {{Xiapu Min}}},
  author = {Chai, Yuan and Ye, Shihong},
  date = {2022-03},
  journaltitle = {Languages},
  volume = {7},
