@article{sundbergObjectiveCharacterizationPhonation2022,
  title = {Objective {{Characterization}} of {{Phonation Type Using Amplitude}} of {{Flow Glottogram Pulse}} and of {{Voice Source Fundamental}}},
  author = {Sundberg, Johan},
  date = {2022-01-01},
